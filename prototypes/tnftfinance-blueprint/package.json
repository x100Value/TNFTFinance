  "license": "ISC",
  "dependencies": {
    "@ton/blueprint": "^0.24.0",
    "@ton/core": "^0.63.0",
    "@ton/crypto": "^3.3.0",
    "@ton/sandbox": "^0.39.0",
    "@ton/test-utils": "^0.7.0"
  }
}
