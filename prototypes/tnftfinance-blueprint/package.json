{
  "name": "tnftfinance",
  "version": "1.0.0",
  "description": "TNFT Finance - NFT lending protocol on TON",
  "main": "index.js",
  "scripts": {
    "build": "npx tact -c tact.config.json",
    "test": "npm run test:artifacts && npm run test:safety",
